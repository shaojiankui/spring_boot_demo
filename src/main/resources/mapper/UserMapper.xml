<mapper namespace="org.skyfox.spring_boot_demo.mapper.UserMapper" >
    <resultMap id="BaseResultMap" type="org.skyfox.spring_boot_demo.entity.User" >
        <id column="user_id" property="user_id" jdbcType="INTEGER" />
    </resultMap>
